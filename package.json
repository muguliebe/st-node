{
  "name": "st-node",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "rimraf dist/ && babel src -d dist",
    "fix": "eslint src --fix",
    "precommit": "yarn run lint",
    "test": "dotenv cucumber-js -- spec/cucumber/features --require-module @babel/register --require spec/cucumber/features",
    "serve": "yarn run build && dotenv node dist/index.js",
    "watch": "nodemon -w src --exec yarn run serve",
    "temp": "yarn run build && dotenv node src/tt.js",
    "tt": "nodemon -w src --exec yarn run temp"
  },
  "author": "zany",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "cucumber": "^5.0.2",
    "dotenv-cli": "^1.4.0",
    "eslint": "^5.7.0",
    "husky": "^1.1.2",
    "ndb": "^1.0.26",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "superagent": "^4.0.0-beta.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4"
  }
}
